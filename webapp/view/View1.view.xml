									<Input id="Cyclename" width="200px"/>
									<Label id="_IDGenLabel9" text="Reference Doc Type"/>
									<Input id="Refdoctype" width="200px"/>
									<core:Title id="_IDGenTitle3"/>
									<Label id="_IDGenLabel10" text="Reference Document"/>
									<Input id="RefDoc" width="200px"/>
									<Label id="_IDGenLabel11" text="NC detected at"/>
									<Input id="NCdec" width="200px"/>
									<Label id="_IDGenLabel12" text="user id"/>
									<Input id="Uid" width="200px"/>
									<Label id="_IDGenLabel13" text="Date created"/>
