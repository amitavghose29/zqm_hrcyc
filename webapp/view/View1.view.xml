							<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
							<Label id="_IDGenLabel1" text="Tracking Number"/>
							<Input id="trackNumber" width="200px"/>
							<ToolbarSpacer id="_IDGenToolbarSpacer2"/>
						</OverflowToolbar>
					
							<f:SimpleForm id="HourCycle123" editable="true" layout="ResponsiveGridLayout" >
								<f:content>
									<core:Title id="_IDGenTitle2"/>
									<Label id="_IDGenLabel2" text="Tracking Number"/>
									<Input id="trackNum" width="200px"/>
									<Label id="_IDGenLabel3" text="Status"/>
