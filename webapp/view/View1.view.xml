									<Label id="_IDGenLabel15" text="Date of last change"/>
									<Input id="changeddate" width="200px"/>
									<Label id="_IDGenLabel16" text="Snag No"/>
									<Input id="snagnum" width="200px"/>
									<Label id="_IDGenLabel17" text="comments"/>
									<Input id="String" width="200px"/>
								</f:content>
							</f:SimpleForm>
						</VBox>
					</content>
					<footer>
						<OverflowToolbar id="otbFooter">
							<ToolbarSpacer id="_IDGenToolbarSpacer3"/>
							<Button id="_IDGenButton1" text="Save" type="Emphasized">
								<layoutData><OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData1" priority="NeverOverflow"/></layoutData>
							</Button>
							<Button id="_IDGenButton2" text="Cancel" type = "Emphasized">
								<layoutData><OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData2" priority="NeverOverflow"/></layoutData>
							</Button>
						</OverflowToolbar>
					</footer>
				</Page>
			</pages>
		</App>
