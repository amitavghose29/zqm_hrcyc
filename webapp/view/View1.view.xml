<mvc:View controllerName="com.airbus.zqmhrcyc.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
	xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}" titleAlignment="Center">
				<!--<headerContent>
						<Image src="./image/airbus.png" height="20px" width="50px"/>
						<link> rel="stylesheet" href="style.css</link>
					</headerContent>-->
					<content> 
						<VBox id="_IDGenVBox1" >
					<Title id="_IDGenTitle1" text="Selection" textAlign="Center"/>
